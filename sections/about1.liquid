      },
      {
        "type":"image_picker",
        "id":"brand-logo-two",
        "label":"Second Brand"
      },
      {
        "type":"image_picker",
        "id":"brand-logo-three",
        "label":"Third Brand"
      },
      {
        "type":"image_picker",
        "id":"brand-logo-four",
        "label":"Four Brand"
      }
    ],
    "presets":[
      {
        "name":"About Section"
      }
    ]
  }
{% endschema %}
